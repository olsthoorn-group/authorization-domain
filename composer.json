{
  "name": "og/authorization-domain",
  "description": "Domain layer code for the OG Authorization library",
  "keywords": [
    "og",
    "olsthoorn-group",
    "authorization-domain"
  ],
  "homepage": "https://github.com/olsthoorn-group/authorization-domain",
  "license": "MIT",
  "authors": [
    {
      "name": "Mitchell Olsthoorn",
      "homepage": "https://github.com/mitchellolsthoorn",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5",
    "og/core-domain": "dev-develop",
    "beberlei/assert": "^2.4"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.0",
    "fabpot/php-cs-fixer": "~1.10",
    "satooshi/php-coveralls": "~0.6",
    "scrutinizer/ocular": "~1.0"
  },
  "suggest": {
  },
  "autoload": {
    "psr-4": {
      "OG\\Authoriation\\Domain\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OG\\Authorization\\Test\\Domain\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "0.1-dev"
    }
  }
}
